class Tree:
    def move(self):
        print "Tree moved"

apple_tree = Tree()

class Creature:
    def move(self):
        print "Creature moved"

class Cat(Creature):
    def move(self):
        print "Cat moved"

def make_it_move(creature):
    if isinstance(creature, Creature):
        print creature.move()

#phoebe = Cat()
make_it_move(Cat())
make_it_move(Tree())

